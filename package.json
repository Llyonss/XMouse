{
  "name": "XMouse",
  "displayName": "XMouse",
  "description": "let's code by mouse",
  "version": "0.4.12",
  "license": "MIT",
  "publisher": "dustycoder",
  "icon": "assets/x.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Llyonss/XMouse.git"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "xmouse.lego.open",
        "command": "xmouse.lego.open"
      },
      {
        "title": "Xmouse:新增组件",
        "command": "xmouse.lego.add"
      },
      {
        "command": "xmouse.lego.list.add",
        "title": "添加组件",
        "icon": {
          "light": "assets/add.light.svg",
          "dark": "assets/add.dark.svg"
        }
      },
      {
        "command": "xmouse.lego.list.import",
        "title": "导入"
      },
      {
        "command": "xmouse.lego.list.export",
        "title": "导出"
      },
      {
        "command": "xmouse.lego.list.save",
        "title": "保存到项目中"
      },
      {
        "command": "xmouse.lego.list.multi-delete",
        "title": "批量删除"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "xmouse",
          "title": "xmouse",
          "icon": "assets/x.png"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "xmouse.lego.add",
          "group": "navigation"
        }
      ],
      "view/title": [
        {
          "command": "xmouse.lego.list.save",
          "when": "view == xmouse.lego.list"
        },
        {
          "command": "xmouse.lego.list.add",
          "when": "view == xmouse.lego.list",
          "group": "navigation"
        },
        {
          "command": "xmouse.lego.list.export",
          "when": "view == xmouse.lego.list"
        },
        {
          "command": "xmouse.lego.list.import",
          "when": "view == xmouse.lego.list"
        },
        {
          "command": "xmouse.lego.list.multi-delete",
          "when": "view == xmouse.lego.list"
        }
      ]
    },
    "views": {
      "xmouse": [
        {
          "type": "webview",
          "id": "xmouse.lego.list",
          "name": "代码大全"
        }
      ]
    }
  },
  "scripts": {
    "install:all": "npm install && cd webview-ui && npm install",
    "start:webview": "cd webview-ui && npm run start",
    "build:webview": "cd webview-ui && npm run build",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "vscode:prepublish": "npm run package",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.5",
    "@types/glob": "^7.1.3",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "prettier": "^2.2.1",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.25.13",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/core": "^7.24.4",
    "@babel/generator": "^7.23.6",
    "@babel/node": "^7.23.9",
    "@babel/parser": "^7.23.9",
    "@babel/traverse": "^7.23.9",
    "@babel/types": "^7.23.9",
    "@react-docgen/cli": "^2.0.3",
    "@vue/compiler-dom": "^3.4.21",
    "ignore": "^5.3.1",
    "react-docgen": "^7.0.3",
    "vue-template-compiler": "^2.7.16"
  }
}