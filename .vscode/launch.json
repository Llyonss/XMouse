{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceRoot}"],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/out/extension.js"
      ],
      "preLaunchTask": "npm: dev"
    }
  ]
}
